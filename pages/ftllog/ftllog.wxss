/* pages/ftllog/ftllog.wxss */
@import "../../components/public/public.wxss";
.ftllog{
  background: #F5F5F5;
  overflow: hidden;
}
.ftl_contral{
  font-size: 26rpx;
}
.ftlcon_title{
  background: #fff;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 50rpx;
  color: #2a3471;
  font-size: 36rpx;
}
.ftlcon_box{
  width: 100%;
  background: #fff;
  overflow: hidden;
  margin-top: 10rpx;
}
.ftlcon_con{
  width: 90%;
  overflow: hidden;
  margin: 20rpx auto 0;
}
.ftlcon_con :first-child{
  border: 0;
}
.row_title{
  background: #bfd3ec;
}
.row{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  display: flex;
  justify-content: space-around;
  text-align: center;
  border-bottom: 1rpx solid #ccc;
}
.row :first-child{
  width: 190rpx;
}
/* .row :nth-child(2){
  width: 60rpx;
}
.row :nth-child(3){
  width: 60rpx;
} */
.row :nth-child(4){
  width: 160rpx;
}
.row :nth-child(5){
  width: 110rpx;
}
