/* pages/workbench/workbench.wxss */
.workbench{
  background: #F5F5F5;
  overflow: hidden;
  position: relative;
}
.bg{
 height: 100vh;
  width: 100vw;
  display: block;
}
.ceshi_swiper {
  width: 550rpx;
  margin: 10rpx auto;
  position: absolute;
  top: 20rpx;
  left: 100rpx;
  height: 900rpx;
  /* overflow: hidden; */
}
.read_swiper{
  width: 100%;
  height: 100%;
}
