.type_name{
  display: block;
  width: 100%;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  margin-top: 20rpx;
}
.slide-image{
  width: 100%;
  height: 100%;
  margin-top: 80rpx;
  position: absolute;
}
/* 箭头 */
.ceshi_next {
  position: absolute;
  top: 30rpx;
  right: 30rpx;
}
.ceshi_prew {
  position: absolute;
  top: 30rpx;
  left: 30rpx;
}
.bench{
  width: 100%;
  height: 750rpx;
  position: absolute;
  top: 150rpx;
  left: 0;
  padding: 25rpx;
  padding-bottom: 0;
  /* background: pink; */
  box-sizing: border-box;
}
.bench text{
  display: block;
  width: 100%;
  text-align: center;
  font-size: 25rpx;
  line-height: 35rpx;
  color: #484E75;
}
.bench text:first-child{
  font-size: 35rpx;
  color: #1D3371;
  line-height: 60rpx;
}
.bench_btn{
  width: 100rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: #409EFE;
  color: #fff;
  border-radius: 5rpx;
  text-align: center;
}



.onebtn,
.twobtn,
.threebtn,
.fourbtn,
.fivebtn,
.sixbtn,
.sevenbtn,
.eightbtn,
.ninebtn,
.tenbtn {
