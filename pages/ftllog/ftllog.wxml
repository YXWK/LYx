<!--pages/ftllog/ftllog.wxml-->
<view class="ftllog">
  <view>
    <image class="bg" src="../../static/top-bg.jpg" mode=""></image>
	</view>
  <view class="ftl_contral">
    <view class="ftlcon_title">水肥机控制</view>
    <view class="ftlcon_box">
      <view class="ftlcon_con">
        <view class="row row_title">
          <text>设备</text>
          <text>状态</text>
          <text>操作</text>
          <text>时间</text>
          <text>备注</text>
        </view>
        <view class="row" wx:for="{{log}}" wx:key="item">
          <text>{{item.name}}</text>
          <text>{{item.isoff}}</text>
          <text>{{item.isseccess}}</text>
          <text>{{item.data}}</text>
          <text>{{item.istimeout}}</text>
        </view>
      </view>
    </view>
  </view>
  <tab/>
</view>
